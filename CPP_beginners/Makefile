# Test Makefile

hellorun: helloworld.cpp
	g++ -o ./build/helloworld helloworld.cpp
	./build/helloworld

ifrun: if.cpp
	g++ -o ./build/if if.cpp
	./build/if

ifelserun: ifelse.cpp
	g++ -o ./build/ifelse ifelse.cpp
	./build/ifelse

whilerun: while.cpp
	g++ -o ./build/while while.cpp
	./build/while

forrun: for.cpp
	g++ -o ./build/for for.cpp
	./build/for
	
arrayrun: array.cpp
	g++ -o ./build/array array.cpp
	./build/array

switchrun: switch.cpp
	g++ -o ./build/switch switch.cpp
	./build/switch

headerrun: header.cpp
	g++ -o ./build/header header.cpp
	./build/header

classesrun: classes.cpp
	g++ -o ./build/classes classes.cpp cat.cpp
	./build/classes

ssrun: stringstreams.cpp
	g++ -o ./build/stringstreams stringstreams.cpp
	./build/stringstreams

	