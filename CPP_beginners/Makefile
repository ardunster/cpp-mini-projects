# Test Makefile

hellorun: helloworld.cpp
	g++ -o ./build/helloworld helloworld.cpp
	./build/helloworld

ifrun: if.cpp
	g++ -o ./build/if if.cpp
	./build/if

ifelserun: ifelse.cpp
	g++ -o ./build/ifelse ifelse.cpp
	./build/ifelse

whilerun: while.cpp
	g++ -o ./build/while while.cpp
	./build/while

forrun: for.cpp
	g++ -o ./build/for for.cpp
	./build/for
	
arrayrun: array.cpp
	g++ -o ./build/array array.cpp
	./build/array

	